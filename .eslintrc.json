{
    "env": {
        "node": true,
        "jasmine": true
    },
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-use-before-define": 0,
        "no-plusplus": 0,
        "import/prefer-default-export": 0
    }
}